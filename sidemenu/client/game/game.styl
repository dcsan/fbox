tbody {
  background-color #ccc
  margin 10px
}

.title {
  position absolute
  top 15px
  left 15px
  padding 5px
  border 2px solid black
  background-color #f1f7f0
  color black
  font-family "Oswald"
  font-size 16px
  z-index 20
}


.debug {
  font-family "Courier"
  font-size 12px
  color #33D
}

#content {
  position relative
  border 2px solid black
  width 90%
  
  #alert {
    position absolute
    top 50px
    left 15px
    border 4px solid black
    background-color white
    color black
    border-radius 10px
    padding 10px
    z-index 100
    // display none
  }
  
  #iframeX {
    position absolute
    top 0
    left 0
    margin 2%
    width 96%
    height 96%
    display none
    z-index 20
  }

}
