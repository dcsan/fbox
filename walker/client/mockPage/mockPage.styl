#navBar {
  position fixed
  width 100%
  height 50px
  top 0px
  left 0px
  background-color black
  z-index 10
}

#title {
  position fixed
  top 20px
  left 60px
  color: white
  z-index 20
  font-size 20px
  font-face "Oswald"
}

.mockImg {
  position absolute
  top 50px
  left 0
  width: 360px
  z-index 1
}

#editButton {
  position fixed
  top 0px
  right 0px
  padding 10px
  z-index 20
  height 30px
  width 50px
  color white
  background-color #3e0001
}

#backButton {
  position fixed
  top 0px
  left 0px
  padding 10px
  z-index 20
  height 40px
  width 25px
  color white
  background-color rgba(255,255,255, 0.3)
  // border 1px solid white
  cursor pointer
}
